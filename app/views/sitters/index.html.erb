<div id="sub-header">
  <p><%= params[:location].capitalize %>, from <%= params[:start_date] %> to <%= params[:end_date]%></p>
</div>

<div class="container-fluid">
<div class="row">
<div class="col-xs-12 col-sm-6 col-md-8">
  <% @sitters.each do |sitter| %>

        <div class="card col-sm-6 col-md-6">
          <div class=" col-xs-12 col-md-6 col-sm-6">
            <%= image_tag "http://fillmurray.com/200/200" %>
          </div>
          <div class="card-description col-xs-12 col-md-6 col-sm-6">
            <ul>
              <li><h4><%= link_to sitter.user.first_name, user_path(sitter) %></h4></li>
              <li><%= sitter.user.city %></li>
              <li>Missions: <%= sitter.missions %></li>
              <br><br><br>
              <li>Fare: $<%= sitter.fare %></li>
            </ul>
          </div>
        </div>


  <% end %>
  </div>
      <div class="col-md-4">
        <div id="map" style="width: 100%; height: 400px;"></div>
      </div>
      </div>

  <!-- <div class="map">
    <div class="row">

    </div>
  </div> -->

</div>

